
function  [X] =  WNNM( Y, C, NSig, m, Iter )
    [U,SigmaY,V] =   svd(full(Y),'econ');    
    PatNum       = size(Y,2);
    TempC  = C*sqrt(PatNum)*2*NSig^2;
    [SigmaX,svp] = ClosedWNNM(SigmaY,TempC,eps);        
    if svp>1
    X =  U(:,1:min(svp, size(Y, 1)))*diag(SigmaX)*V(:,1:min(svp, size(Y, 1)))' + m;     
return;
